{
  "extends": "@quasar/app-webpack/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      // "api/*": ["/api/*"]
      "root/*": ["./*"],
      "stores/*": ["./src/stores/*"],
      "types/*": ["./src/types/*"],
      "composables/*": ["./src/composables/*"]
    }
  },
  "noImplicitAny": true,
}
